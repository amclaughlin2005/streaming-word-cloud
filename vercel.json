{
  "version": 2,
  "routes": [
    {
      "src": "/api/wordcloud-data",
      "dest": "/api/wordcloud-data.py"
    },
    {
      "src": "/api/question-types-data", 
      "dest": "/api/question-types-data.py"
    },
    {
      "src": "/api/sentiment-data",
      "dest": "/api/sentiment-data.py"
    },
    {
      "src": "/api/upload-data",
      "dest": "/api/upload-data.js"
    },
    {
      "src": "/api/init-data",
      "dest": "/api/init-data.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/wordcloud-data.py": {
      "maxDuration": 30
    },
    "api/question-types-data.py": {
      "maxDuration": 30
    },
    "api/sentiment-data.py": {
      "maxDuration": 30
    },
    "api/upload-data.js": {
      "maxDuration": 30
    },
    "api/init-data.js": {
      "maxDuration": 30
    },
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "CSV_FILE_PATH": "data/demo_feedback.csv",
    "NODE_ENV": "production"
  }
} 