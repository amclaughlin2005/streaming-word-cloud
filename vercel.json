{
  "version": 2,
  "builds": [
    {
      "src": "api/wordcloud.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/wordcloud-data",
      "dest": "/api/wordcloud.py"
    },
    {
      "src": "/api/question-types-data", 
      "dest": "/api/wordcloud.py?question-types=true"
    },
    {
      "src": "/api/sentiment-data",
      "dest": "/api/wordcloud.py?sentiment=true"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico))",
      "dest": "/public/$1"
    }
  ]
}